{
    "name": "ext",
    "description": "extension",
    "version": "0.1",
    "manifest_version": 2,
    "key": "",
    "icons": {

    },
    "background": {
        "scripts": [
            "./background.js",
            "./jsrsasign-all-min.js"
        ]
    },
    "options_page": "./options.html",
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Simple extension"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/*",
        "https://*/*",
        "storage",
        "identity"
    ]
}